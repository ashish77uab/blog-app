
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Task {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  slug     String? @unique
  title    String
  body     String
  isCompleted Boolean?  @default(false)
}
